{
  "name": "less-to-pallette",
  "version": "1.0.0",
  "description": "webpage to make working with less file colour variables more pleasant",
  "main": "index.js",
  "repository": "https://github.com/ThatCrazyIrishGuy/less-to-palette",
  "scripts": {
    "start": "node_modules/.bin/cross-env NODE_ENV=development node_modules/.bin/webpack-dev-server --colors --config webpack.config.js",
    "test": "jest --coverage",
    "test-ci": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "build": "node_modules/.bin/cross-env NODE_ENV=production node_modules/.bin/webpack -p --config ./webpack.production.config.js",
    "lint": "node_modules/.bin/eslint -c .eslintrc src tests --color",
    "lint-fix": "node_modules/.bin/eslint -c .eslintrc src tests  --color --fix"
  },
  "author": "Darren Britton",
  "license": "ISC",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-plugin-transform-node-env-inline": "^6.8.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-runtime": "^6.23.0",
    "caniuse-db": "^1.0.30000631",
    "chai": "^3.5.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "css-modules-require-hook": "^4.0.5",
    "enzyme": "^2.7.1",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-babel": "~4.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "jest": "^19.0.2",
    "postcss-cssnext": "^2.9.0",
    "postcss-each": "^0.9.3",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.2.2",
    "postcss-mixins": "^5.4.1",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.4.2",
    "redux-devtools-extension": "^1.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.2.0",
    "coveralls": "2.12.0"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "babel-preset-stage-1": "^6.1.18",
    "flexboxgrid": "^6.3.1",
    "identity-obj-proxy": "^3.0.0",
    "postcss": "^8.2.13",
    "postcss-cssnext": "^2.9.0",
    "preact": "^7.2.0",
    "preact-compat": "^3.13.1",
    "promise-file-reader": "^0.3.1",
    "query-string": "^4.3.2",
    "ramda": "^0.23.0",
    "react": "^15.4.2",
    "react-copy-to-clipboard": "^4.2.3",
    "react-css-modules": "^4.1.0",
    "react-dom": "^16.0.1",
    "react-dropzone": "^3.11.0",
    "react-flexbox-grid": "^0.10.2",
    "react-flip-move": "^2.9.1",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.1",
    "react-sparklines": "^1.6.0",
    "react-style-proptype": "^2.0.1",
    "react-toolbox": "^2.0.0-beta.6",
    "redux": "^3.6.0",
    "redux-promise": "^0.5.3",
    "tinycolor2": "^1.4.1"
  }
}
